#!/bin/bash
executable     = ./RunAnSt1_HTC_20GeV_1e-4_testing.sh
universe       = vanilla
arguments      = $(ClusterId) $(ProcId)
output         = DNN12_testing_20GeV_1e-4.$(ClusterId).$(ProcId).out
